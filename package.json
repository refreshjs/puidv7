{
  "name": "@refreshjs/puidv7",
  "version": "1.0.8",
  "description": "Prefixed UUIDv7's in crockford base32 encoding",
  "homepage": "https://github.com/refreshjs/puidv7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/refreshjs/puidv7.git"
  },
  "license": "Apache-2.0",
  "contributors": [
    {
      "name": "Nadraman",
      "url": "https://nadrama.com"
    }
  ],
  "files": [
    "dist/*"
  ],
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "prepare": "bun test && bun run build && bun run types",
    "format": "prettier -w .",
    "ci-format": "prettier -c .",
    "lint": "oxlint",
    "test": "vitest ./src",
    "ci-test": "vitest --run",
    "types": "bun run tsc --emitDeclarationOnly",
    "build": "bun build --outdir dist/ --target=browser $(find src -name '*.ts' ! -name '*.test.ts')",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@scure/base": "^1.2.4",
    "uuidv7": "^1.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "vitest": "^3.1.1",
    "oxlint": "^0.16.6",
    "prettier": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.3.2",
    "drizzle-orm": "^0.36.1"
  },
  "engines": {
    "node": ">=22.9.0"
  }
}
